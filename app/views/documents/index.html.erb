<div class="page-header"><h1>Upload Photo</h1></div>

<div class="media">
  <% @documents.each do |document| %>
    <div class="media-left">
      <!-- Renders image -->
      <%= link_to 'documents', @document.doc.url %>
    </div>
    <div class="media-body">
      <h4 class="media-heading"><%= document.title %></h4>
    </div>
  <% end %>
</div>